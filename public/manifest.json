{
  "manifest_version": 3,
  "name": "OverNewTab",
  "description": "OverNewTab replaces the default new tab with a customizable interface.",
  "version": "1.1.1",
  "action": {},
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "48": "icons/icon.svg",
    "96": "icons/icon.svg"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{7bd3ab37-7c59-4b35-bd79-218bd69f9829}"
    }
  },
  "permissions": ["bookmarks"],
  "host_permissions": ["https://api.weatherapi.com/", "https://ipinfo.io/"],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  }
}

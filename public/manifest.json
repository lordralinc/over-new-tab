{
  "manifest_version": 2,
  "name": "OverNewTab",
  "description": "Расширение OverNewTab заменяет стандартную новую вкладку на кастомизируемый интерфейс",
  "version": "1.1.0",
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "icons": {
    "48": "icons/icon.svg",
    "96": "icons/icon.svg"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["bookmarks"]
}
